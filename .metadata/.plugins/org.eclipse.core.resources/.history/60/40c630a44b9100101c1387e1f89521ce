#include <stdint.h>
#include "lis3dsh_spi_polling_demo.h"
#include <stdio.h>
#include "STM32f407VGT.h"

static inline void FPU_Enable(void)
{
    SCB_CPACR |= (0xF << 20);   // set CP10 = 11 and CP11 = 11 (full access)
    __asm volatile ("dsb");     // Data Synchronization Barrier
    __asm volatile ("isb");     // Instruction Synchronization Barrier
}

int main(void)
{
	 FPU_Enable();
//	lis3dsh_spi_polling_demo_main();
	while(1)
	{
		lis3dsh_spi_polling_demo_main();
	}
	 return 0;
}

